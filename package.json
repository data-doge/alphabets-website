{
  "name": "alphabets-website",
  "version": "1.0.0",
  "description": "website for alphabets",
  "main": "./src/index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2040"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/data-doge/alphabets-website"
  },
  "author": "data-doge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-doge/alphabets-website/issues"
  },
  "homepage": "https://github.com/data-doge/alphabets-website",
  "dependencies": {
    "jquery": "^2.2.3",
    "lodash.range": "^3.2.0",
    "lodash.round": "^4.0.4",
    "lodash.sample": "^4.2.1",
    "raf-loop": "^1.1.3",
    "three": "^0.76.1",
    "three-orbit-controls": "^72.0.0",
    "three-window-resize": "^1.0.4"
  },
  "devDependencies": {
    "babel-preset-es2040": "^1.1.1",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "budo": "^9.4.1",
    "gh-pages": "^0.12.0",
    "uglifyify": "^3.0.4"
  }
}
